{
  "printWidth": 120,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "overrides": [
    {
      "files": [".prettierrc"],
      "options": {
        "printWidth": 60
      }
    }
  ]
}
